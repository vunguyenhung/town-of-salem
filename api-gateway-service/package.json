    "dotenv": "4.0.0",
    "errorhandler": "1.5.0",
    "express": "4.15.2",
    "folktale": "2.0.1",
    "graphql": "0.11.3",
    "graphql-subscriptions": "0.5.4",
    "graphql-tools": "1.2.3",
    "kafka-node": "2.2.3",
    "morgan": "1.8.1",
    "ramda": "0.24.1",
    "rxjs": "5.4.3",
    "subscriptions-transport-ws": "0.9.1",
    "superagent": "3.7.0"
  },
  "devDependencies": {
    "babel-jest": "21.0.2",
    "eslint": "4.7.1",
    "eslint-config-airbnb-base": "12.0.0",
